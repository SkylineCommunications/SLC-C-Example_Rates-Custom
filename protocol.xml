				<RTDisplay>true</RTDisplay>
				<Decimals>3</Decimals>
				<Range>
					<Low>0</Low>
				</Range>
				<Units>bps</Units>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="1007" trending="false">
			<Name>StreamsBitRateOnTimesData</Name>
			<Description>Bit Rate on Times Data (Streams)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Used as a buffer parameter containing data necessary for bit rate calculations based on TimeSpan.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
			</Display>
			<Measurement>
				<Type>string</Type>
			</Measurement>
		</Param>
	</Params>

