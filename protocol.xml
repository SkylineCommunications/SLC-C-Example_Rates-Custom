<Protocol xmlns="http://www.skyline.be/protocol">
	<Name>SLC SDF Rates - Custom</Name>
	<Description>SLC SDF Rates - Custom DataMiner driver</Description>
	<Version>1.0.1.2</Version>
	<IntegrationID>DMS-DRV-0000</IntegrationID>
	<Provider>Skyline Communications</Provider>
	<Vendor>Skyline</Vendor>
	<VendorOID>1.3.6.1.4.1.8813.2.48</VendorOID>
	<DeviceOID>998</DeviceOID>
	<ElementType>Training Example</ElementType>
	<Type relativeTimers="true">virtual</Type>
	<Display defaultPage="General" pageOrder="General;Standalone;Table" wideColumnPages="General;Table"/>
	<SNMP includepages="true">auto</SNMP>

	<Compliancies>
		<CassandraReady>true</CassandraReady>
		<MinimumRequiredVersion>10.0.0.0 - 11025</MinimumRequiredVersion>
	</Compliancies>

	<Params>
		<Param id="2">
			<Name>afterStartup</Name>
			<Description>After Startup</Description>
			<Type>dummy</Type>
		</Param>
		
		<Param id="9" trending="false">
			<Name>ReadMe</Name>
			<Description>Read Me</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Description of this example protocol.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
				<DefaultValue>
					<![CDATA[
This protocol demonstrates how to calculate rates based on counters polled by any means other than SNMP.

- The 'Standalone' page contains a first example on how to calculate rates for standalone parameters which might be easier to have a first look at the rate calculation logic.
- The 'Table' page contains a second example on how to calculate rates on tables.
					]]>
				</DefaultValue>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type lines="10">string</Type>
			</Measurement>
		</Param>
		
		<Param id="90">
			<Name>CounterProcessingQAction</Name>
			<Description>Counter Processing QAction</Description>
			<Type>dummy</Type>
		</Param>
		<Param id="100" trending="true">
			<Name>Counter</Name>
			<Description>Counter</Description>
			<Type>read</Type>
			<Information>
				<Subtext>A standalone octets counter.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<!-- SuppressValidator 2.5.1 Thresholds fully depending on users -->
			<Alarm>
				<Monitored>true</Monitored>
			</Alarm>
			<!-- /SuppressValidator 2.5.1 -->
			<Display>
				<RTDisplay>true</RTDisplay>
				<Units>Units</Units>
				<Range>
					<Low>0</Low>
				</Range>
				<Positions>
					<Position>
						<Page>Standalone</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="101" trending="true">
			<Name>CounterRateOnDates</Name>
			<Description>Counter Rate on Dates</Description>
			<Type>read</Type>
			<Information>
				<Subtext>The calculated counter rate based on DateTime.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
				<Exceptions>
					<Exception id="1" value="-1">
						<Display state="disabled">N/A</Display>
						<Value>-1</Value>
					</Exception>
				</Exceptions>
			</Interprete>
			<!-- SuppressValidator 2.5.1 Thresholds fully depending on users -->
			<Alarm>
				<Monitored>true</Monitored>
			</Alarm>
			<!-- /SuppressValidator 2.5.1 -->
			<Display>
				<RTDisplay>true</RTDisplay>
				<Decimals>3</Decimals>
				<Units>Units/s</Units>
				<Range>
					<Low>0</Low>
				</Range>
				<Positions>
					<Position>
						<Page>Standalone</Page>
						<Column>0</Column>
						<Row>2</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type>number</Type>
			</Measurement>
		</Param>
		<Param id="102" trending="false" save="true">
			<Name>CounterRateOnDatesData</Name>
			<Description>Counter Rate on Dates Data</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Used as a buffer parameter containing data necessary for counter rate calculations based on DateTime.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>Standalone</Page>
						<Column>0</Column>
						<Row>3</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type lines="10">string</Type>
			</Measurement>
		</Param>
		
		<!-- Streams Table -->
		<Param id="990">
			<Name>StreamsProcessingQAction</Name>
			<Description>Streams Processing QAction</Description>
			<Type>dummy</Type>
		</Param>
		<Param id="1000">
			<Name>Streams</Name>
			<Description>Streams</Description>
			<Type>array</Type>
			<ArrayOptions index="0">
				<ColumnOption idx="0" pid="1001" type="retrieved" options=""/>
				<ColumnOption idx="1" pid="1002" type="retrieved" options=";save" />
				<ColumnOption idx="2" pid="1003" type="retrieved" options=";save;disableHeaderSum;disableHistogram;disableHeatmap" />
				<ColumnOption idx="3" pid="1004" type="retrieved" options=";disableHeaderSum;disableHistogram;disableHeatmap" />
				<ColumnOption idx="4" pid="1005" type="retrieved" options=";save" />
				<ColumnOption idx="5" pid="1006" type="retrieved" options=";disableHeaderSum;disableHistogram;disableHeatmap" />
				<ColumnOption idx="6" pid="1007" type="retrieved" options=";save" />
			</ArrayOptions>
			<Information>
				<Subtext>Streams Table</Subtext>
			</Information>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>Table</Page>
						<Column>0</Column>
						<Row>0</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type options="tab=columns:1001|0-1002|1-1003|2-1005|4-1004|3-1007|6-1006|5,width:97-275-130-0-140-0-140,sort:STRING-STRING-INT-STRING-INT-STRING-INT,lines:25,filter:true">table</Type>
			</Measurement>
		</Param>
		<Param id="1001" trending="false">
			<Name>StreamsIndex</Name>
			<Description>Index [IDX] (Streams)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>This is the key used internally by DataMiner to identify the table entries.</Subtext>
			</Information>
